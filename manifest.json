{
  "manifest_version": 2,
  "name": "Statsee Graph Describer",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["jquery-3.2.1.js", "background.js"]
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
  "permissions": [
    "http://127.0.0.1:5000/*"
  ]
}
